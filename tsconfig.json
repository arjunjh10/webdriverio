{
    "compilerOptions": {
      "outDir": "./.tsbuild/",
      "sourceMap": false,
      "target": "es2019",
      "module": "commonjs",
      "removeComments": true,
      "noImplicitAny": true,
      "strictPropertyInitialization": true,
      "suppressImplicitAnyIndexErrors": true,
      "strictNullChecks": true,
      "types": [
        "node",
        "webdriverio/async",
        "@wdio/cucumber-framework"
      ]
    }
  }